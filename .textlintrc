{
  "filters": {},
  "rules": {
    "preset-ja-spacing": {
      "ja-space-between-half-and-full-width": {
        "space": "always"
      }
    },
    "preset-ja-technical-writing": true,
    "spellcheck-tech-word": true,
    "ja-technical-writing/ja-no-mixed-period": {
      "allowPeriodMarks": [":"]
    },
    "prh": {
      "rulePaths": [
        "./prh.yml"
      ]
    }
  }
}
